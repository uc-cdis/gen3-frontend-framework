{
  "name": "@gen3/toolsff",
  "version": "0.10.81",
  "description": "tools for processing Gen3 commons content: color theme, icons",
  "main": "index.js",
  "type": "module",
  "engines": {
    "npm": ">=10.8.2",
    "node": ">=20.11.0"
  },
  "scripts": {
