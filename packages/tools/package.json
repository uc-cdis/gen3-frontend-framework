    "undici": "^6.21.1"
  },
  "devDependencies": {
    "rollup-plugin-executable": "^1.6.3"
  },
  "peerDependencies": {
    "ts-node": "^10.9.2"
  },
  "files": [
    "dist"
  ],
  "overrides": {
    "@iconify/tools": {
      "cheerio": "^1.0.0-rc.12",
