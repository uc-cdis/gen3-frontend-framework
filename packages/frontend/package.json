{
  "name": "@gen3/frontend",
  "version": "0.1.13",
  "description": "Gen3 frontend components, content management, and pages",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "clean": "rimraf dist",
    "test": "jest unit",
    "test:watch": "jest unit --watch",
    "test:int": "echo No integrations tests yet",
    "test:all": "jest",
    "types": "tsc --emitDeclarationOnly",
    "copy-tailwind": "mkdir -p dist && cp src/tailwind.cjs dist/tailwind.cjs",
    "copy-style": "mkdir -p dist/styles && cp src/styles/* dist/styles/",
    "build:clean": "npm run clean && npm run build:swc && npm run types",
    "build:pkg": "rollup --config rollup.config.mjs",
    "build:swc": "npm run copy-tailwind && swc src -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "Apache-2.0",
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./*": [
      "./dist/*.js",
      "./dist/*/index.js"
    ],
    "./tailwind": {
      "require": "./dist/tailwind.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ],
      "tailwind": [
        "dist/tailwind.d.cts"
      ]
    }
  },
  "publishConfig": {
    "typesVersions": {
      "*": {
        "*": [
          "dist/*.d.ts",
          "dist/*/index.d.ts"
        ]
      }
    }
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.5.12",
    "@fontsource/source-sans-pro": "^4.5.11",
    "@gen3/core": "0.1.0",
    "@iconify/types": "^2.0.0",
    "@mantine/core": "^6.0.4",
    "@mantine/hooks": "^6.0.4",
    "@mantine/next": "^6.0.4",
    "@mantine/notifications": "^6.0.4",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/mdx": "^2.1.5",
    "@next/mdx": "^13.2.3",
    "@reduxjs/toolkit": "^1.9.3",
    "chart.js": "^4.0.1",
    "cookies-next": "^2.1.1",
    "fs": "^0.0.1-security",
    "gray-matter": "^4.0.3",
    "jose": "^4.13.1",
    "js-cookie": "^3.0.1",
    "mantine-react-table": "^v1.0.0-beta.7",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.5",
    "postcss": "^8.4.24",
    "postcss-loader": "^7.3.2",
    "postcss-preset-env": "^8.4.2",
    "react-chartjs-2": "^5.2.0",
    "react-cookie": "^4.1.1",
    "react-icons": "^4.7.1",
    "react-plotly.js": "^2.5.1",
    "react-redux": "^8.0.7",
    "react-table": "^7.8.0",
    "recharts": "^2.4.3",
    "sharp": "^0.30.7",
    "swr": "^2.1.5",
    "tailwind-styled-components": "^2.2.0",
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "html-webpack-plugin": "^5.5.0",
    "@iconify/react": "^4.0.1",
    "@iconify/tools": "^2.1.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@svgr/webpack": "^8.0.1",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.62",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/color": "^3.0.3",
    "@types/estree": "^1.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/mdx": "^2.0.3",
    "@types/recharts": "^1.8.23",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "18.15.5",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.0.11",
    "@types/react-table": "^7.7.12",
    "@types/tinycolor2": "^1.4.3",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "autoprefixer": "^10.4.14",
    "css-loader": "^6.7.2",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "jest": "^29.5.0",
    "rollup": "^3.24.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "style-loader": "^3.3.2",
    "tailwindcss": "^3.2.4",
    "ts-jest": "^29.0.3",
    "next": "^13.4.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@mantine/core": "^6.0.4",
    "@mantine/hooks": "^6.0.4",
    "typescript": "5.0.2"
  }
}
