    "react-chartjs-2": "^5.2.0",
    "react-cookie": "^4.1.1",
    "react-icons": "^4.10.1",
    "react-plotly.js": "^2.5.1",
    "react-redux": "^8.0.7",
    "react-table": "^7.8.0",
    "recharts": "^2.4.3",
    "sharp": "^0.30.7",
    "swr": "^2.1.5",
    "tailwind-styled-components": "^2.2.0",
    "tinycolor2": "^1.6.0"
