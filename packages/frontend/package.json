    "test": "jest unit",
    "test:watch": "jest unit --watch",
    "test:int": "echo No integrations tests yet",
    "test:all": "jest",
    "types": "tsc --emitDeclarationOnly",
    "copy-tailwind": "cp src/tailwind.cjs dist/tailwind.cjs",
