{
  "name": "@gen3/frontend",
  "version": "0.1.13",
  "description": "Gen3 frontend components, content management, and pages",
  "keywords": [],
  "author": "CTDS",
  "license": "Apache-2.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "clean": "rimraf dist",
    "test": "jest unit",
    "test:watch": "jest unit --watch",
    "test:int": "echo No integrations tests yet",
    "test:all": "jest",
    "types": "tsc --emitDeclarationOnly",
    "copy-tailwind": "mkdir -p dist && cp src/tailwind.cjs dist/tailwind.cjs",
    "copy-style": "mkdir -p dist/styles && cp src/styles/* dist/styles/",
    "build": "npm run clean && npm run build:rollup && npm run types",
    "build:rollup": "npm run compile && npm run copy-tailwind && rollup --config rollup.config.mjs",
    "build:swc": "npm run compile && npm run copy-tailwind && swc src -d dist",
    "build:watch": "npm run compile && npm run build:swc -- --watch",
    "dev": "npm run build:watch"
  },
  "exports": {
    "." : "./dist/index.js",
    "./tailwind": {
      "require": "./dist/tailwind.cjs"
    }
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "dependencies": {
    "@fontsource/montserrat": "^4.5.12",
    "@fontsource/source-sans-pro": "^4.5.11",
    "@gen3/core": "0.1.0",
    "@graphiql/plugin-explorer": "^0.3.4",
    "@graphiql/react": "^0.19.3",
    "@iconify/types": "^2.0.0",
    "@mantine/core": "^6.0.20",
    "@mantine/hooks": "^6.0.20",
    "@mantine/next": "^6.0.20",
    "@mantine/notifications": "^6.0.20",
    "@mdx-js/loader": "^2.1.5",
    "@mdx-js/mdx": "^2.1.5",
    "@next/mdx": "^13.4.19",
    "@reduxjs/toolkit": "^1.9.3",
    "chart.js": "^4.0.1",
    "cookies-next": "^2.1.1",
    "fs": "^0.0.1-security",
    "graphiql": "^3.0.5",
    "graphql": "^16.8.0",
    "graphql-ws": "^5.14.0",
    "gray-matter": "^4.0.3",
    "jose": "^4.13.1",
    "js-cookie": "^3.0.5",
    "jsonpath-plus": "^7.2.0",
    "mantine-react-table": "^1.2.1",
    "next-compose-plugins": "^2.2.1",
    "next-images": "^1.8.5",
