{
  "name": "@gen3/core",
  "version": "0.10.72",
  "author": "CTDS",
  "description": "Core module for Gen3.2. Packages provides an interface for interacting with the gen3 API and a redux store for managing state.",
  "license": "Apache-2.0",
  "engines": {
    "npm": ">=10.8.2",
    "node": ">=20.11.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "types": "tsc --emitDeclarationOnly --skipLibCheck",
    "build": "npm run clean && npm run compile && rollup --config rollup.config.mjs",
    "build:clean": "npm run clean && npm run compile && npm run types && rollup --config rollup.config.mjs",
    "build:watch": "npm run compile && npm run build -- --watch",
    "test": "jest unit",
    "test:watch": "jest unit --watch",
    "test:int": "jest int",
    "test:all": "jest",
    "dev": "npm run build:watch"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-cookie": "^7.2.2",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@swc/wasm": "^1.3.85",
    "@testing-library/react": "^14.0.0",
    "@types/estree": "^1.0.0",
    "@types/fastestsmallesttextencoderdecoder": "^1.0.2",
    "@types/flat": "^5.0.3",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/lodash": "^4.14.191",
    "@types/papaparse": "^5.3.14",
    "@types/uuid": "^9.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.5.0",
    "flat": "^6.0.1",
    "papaparse": "^5.4.1",
    "queue": "6.0.2",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "swr": "^2.3.0",
    "uuid": "^9.0.1"
  },
  "files": [
    "dist"
  ]
}
