    "rimraf": "^6.0.1",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-swc3": "^0.10.2",
    "rollup-swc-preserve-directives": "^0.5.0",
    "terser-webpack-plugin": "^5.3.3",
