    "@mantine/notifications": "^7.12.1",
    "classnames": "^2.3.1",
    "dom-to-svg": "^0.12.2",
    "isomorphic-fetch": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-responsive-carousel": "^3.2.23",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.0",
    "@next/eslint-plugin-next": "^14.2.3",
