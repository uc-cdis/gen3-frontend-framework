    "ts-json-schema-generator": "^2.3.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.2"
  },
  "overrides": {
    "rollup-plugin-copy": {
      "globby": "^11.0.0"
    }
  },
  "optionalDependencies": {
    "@nx/nx-darwin-arm64": "^20.3.0",
    "@nx/nx-linux-arm64-gnu": "^20.3.0",
    "@nx/nx-linux-x64-gnu": "^20.3.0"
  }
